    "graphql-shield": "^7.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.7",
    "nodemon": "^1.19.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/app-module-path": "^2.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.2",
